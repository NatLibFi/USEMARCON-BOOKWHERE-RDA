                                 RegReplace('\\bS\\b', 'sopraano', 'g'); \
                                 RegReplace('\\bMz\\s*(\\(\\d+)', 'mezzosopraanot \\1', 'g'); \
                                 RegReplace('\\bMz\\b', 'mezzosopraano', 'g'); \
